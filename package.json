{
  "name": "@occmundial/occ-atomic",
  "version": "0.3.4",
  "description": "Collection of shareable styled React components for OCC applications.",
  "homepage": "http://occmundial.github.io/occ-atomic",
  "main": "build/index.js",
  "scripts": {
    "prestart": "npm run gen-docs",
    "start": "npm-run-all --parallel start-docs gen-docs:watch",
    "start-docs": "node scripts/server.js",
    "gen-docs": "node scripts/generateComponentData.js",
    "gen-docs:watch": "npm run gen-docs -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "babel lib -d build",
    "build:watch": "babel lib -w -d build",
    "build:bundle": "babel-node scripts/build.js",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepare": "npm run build",
    "predeploy": "set NODE_ENV=production && npm run build:bundle",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/occmundial/occ-atomic"
  },
  "author": "Luis DÃ­az",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^2.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "gh-pages": "^1.2.0",
    "jest": "^22.4.3",
    "jsdom": "^11.11.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.3.1",
    "style-loader": "^0.21.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "jquery": "^3.2.1",
    "react-jss": "^8.2.1",
    "react-waypoint": "^8.0.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "roots": [
      "<rootDir>/lib/"
    ]
  }
}
